  "bugs": {
    "url": "https://github.com/tomfletch/fletcher-fashion/issues"
  },
  "homepage": "https://github.com/tomfletch/fletcher-fashion#readme",
  "devDependencies": {
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22"
  },
  "dependencies": {
    "express": "^4.18.2"
  }
}
